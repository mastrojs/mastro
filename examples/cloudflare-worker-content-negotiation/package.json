{
	"name": "my-first-worker",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "deno serve --watch=. --allow-read --allow-env server.ts",
		"dev": "pnpm run generate && wrangler dev",
