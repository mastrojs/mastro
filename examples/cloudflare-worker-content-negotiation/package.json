		"cf-typegen": "wrangler types"
	},
	"engines": {
		"node": ">=24.12"
	},
	"dependencies": {
		"@mastrojs/markdown": "jsr:^0.1.4",
		"@mastrojs/mastro": "jsr:^0"
	},
	"devDependencies": {
		"typescript": "^5.5.2",
		"wrangler": "^4.56.0"
	}
}
